{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "**.csproj"
          ],
          "exclude": [
            "docfx_project/**",
            "*Tests/**",
            "IndicoV2*/**",
            "Examples/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "outputFormat": "markdown"
    },
    {
      "src": [
        {
          "src": "../",
          "files": [
            "IndicoV2*/**.csproj"
          ],
          "exclude": [
            "docfx_project/**",
            "*Tests/**",
            "IndicoV2.V1Adapters/**"
          ]
        }
      ],
      "dest": "apiV2",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "outputFormat": "markdown"
    }
  ],
  "build": {
    "content": [
      {
        "src": "../",
        "files": "README.md",
        "dest": "api"
      },
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "apiV2/**.yml",
          "apiV2/index.md"
        ]
      },
      {
        "files": [
          "examplesV2/**.md",
          "examplesV2/**.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}